<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="viewModel"
            type="com.qiyao.bysj.runningisthebest.module.moment.viewmodel.MomentItemViewModel" />
    </data>
    <LinearLayout
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/view_moment_content"
            app:viewModel="@{viewModel.contentViewModel}"/>
        <include layout="@layout/view_moment_likes_and_comments"
            app:viewModel="@{viewModel.likesAndCommentsViewModel}"/>
    </LinearLayout>
</layout>
